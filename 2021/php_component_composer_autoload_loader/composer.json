{
    "name": "net_bazzline/php_component_composer_autoload_loader",
    "description": "free as in freedom php component to easy dealing with composer autoload",
    "keywords": [
        "bazzline",
        "psr",
        "psr-4",
        "php", 
        "php56", 
        "php7", 
        "component", 
        "composer", 
        "autoload", 
        "lgpl", 
        "free as in freedom"
    ],
    "type": "library",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "mikey179/vfsstream": "1.6.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "5.7.*"
    },
    "license": "LGPL-3.0",
    "authors": [
        {
            "name": "Stev Leibelt",
            "email": "artodeto@bazzline.net",
            "homepage": "https://artodeto.bazzline.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Net\\Bazzline\\Component\\ComposerAutoload\\": "source/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Net\\Bazzline\\Component\\ComposerAutoload\\": "test/"
        }
    }
}
