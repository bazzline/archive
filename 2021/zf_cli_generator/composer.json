{
    "description": "free as in freedom zend framework 2 cli generator module",
    "keywords": [
        "bazzline",
        "zf2",
        "zend framework 2",
        "module",
        "php",
        "cli",
        "configuration",
        "generator",
        "lgpl",
        "free as in freedom",
        "readline",
        "autocomplete",
        "console"
    ],
    "license": "LGPL-3.0",
